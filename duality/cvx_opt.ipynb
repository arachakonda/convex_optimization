   "source": [
    "import cvxpy as cp\n",
    "\n",
    "x = cp.Variable()\n",
    "objective = cp.Minimize(x**4-4*x**3-6*x**2+200)\n",
    "constraints = [x>=24]\n",
    "prob = cp.Problem(objective, constraints)\n",
    "result = prob.solve()\n",
    "print(\"Optimal value: \", result)\n",
    "print(\"Optimal var: \", x.value)"
   ]
